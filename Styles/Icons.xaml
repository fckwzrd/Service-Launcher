<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- Info Icon -->
    <Viewbox x:Key="InfoIcon" Width="24" Height="24" Stretch="Uniform">
        <Canvas Width="24" Height="24">
            <Rectangle Width="24" Height="24" Fill="Transparent"/>
            <Ellipse Stroke="#E31E24" StrokeThickness="2"
                     Width="20" Height="20" Canvas.Left="2" Canvas.Top="2"/>
            <Path Fill="#E31E24" 
                  Data="M12 7.5C12.8284 7.5 13.5 6.82843 13.5 6C13.5 5.17157 12.8284 4.5 12 4.5C11.1716 4.5 10.5 5.17157 10.5 6C10.5 6.82843 11.1716 7.5 12 7.5Z"/>
            <Path Stroke="#E31E24" StrokeThickness="2" StrokeStartLineCap="Round" StrokeEndLineCap="Round"
                  Data="M12 9.5V18"/>
        </Canvas>
    </Viewbox>

    <!-- Password Icon -->
    <Viewbox x:Key="PasswordIcon" Width="24" Height="24" Stretch="Uniform">
        <Canvas Width="24" Height="24">
            <Rectangle Width="24" Height="24" Fill="Transparent"/>
            <Path Stroke="#E31E24" StrokeThickness="2" StrokeLineJoin="Round"
                  Data="M4 11C4 9.89543 4.89543 9 6 9H18C19.1046 9 20 9.89543 20 11V19C20 20.1046 19.1046 21 18 21H6C4.89543 21 4 20.1046 4 19V11Z"/>
            <Path Stroke="#E31E24" StrokeThickness="2" StrokeLineJoin="Round"
                  Data="M8 9V7C8 4.79086 9.79086 3 12 3C14.2091 3 16 4.79086 16 7V9"/>
            <Ellipse Fill="#E31E24"
                     Width="4" Height="4" Canvas.Left="10" Canvas.Top="14"/>
        </Canvas>
    </Viewbox>

    <!-- Email Icon -->
    <Viewbox x:Key="EmailIcon" Width="24" Height="24" Stretch="Uniform">
        <Canvas Width="24" Height="24">
            <Rectangle Width="24" Height="24" Fill="Transparent"/>
            <Path Stroke="#E31E24" StrokeThickness="2" StrokeLineJoin="Round"
                  Data="M3 6C3 4.89543 3.89543 4 5 4H19C20.1046 4 21 4.89543 21 6V18C21 19.1046 20.1046 20 19 20H5C3.89543 20 3 19.1046 3 18V6Z"/>
            <Path Stroke="#E31E24" StrokeThickness="2" StrokeLineJoin="Round"
                  Data="M3 6L10.2 12.65C11.2667 13.45 12.7333 13.45 13.8 12.65L21 6"/>
        </Canvas>
    </Viewbox>

    <!-- User Icon -->
    <Viewbox x:Key="UserIcon" Width="24" Height="24" Stretch="Uniform">
        <Canvas Width="24" Height="24">
            <Rectangle Width="24" Height="24" Fill="Transparent"/>
            <Ellipse Stroke="#E31E24" StrokeThickness="2"
                     Width="10" Height="10" Canvas.Left="7" Canvas.Top="3"/>
            <Path Stroke="#E31E24" StrokeThickness="2" StrokeLineJoin="Round"
                  Data="M5 20C5 17.2386 7.23858 15 10 15H14C16.7614 15 19 17.2386 19 20"/>
        </Canvas>
    </Viewbox>

    <!-- Eye Icon (Show Password) -->
    <!-- Eye Off Icon (Hide Password) -->

</ResourceDictionary> 